@model IEnumerable<EncryptedCookie.Models.Post>

@{
    ViewData["Title"] = "Home Page";
}

<form method="get">
    <input type="search" name="searchTerm" />
    <input type="submit" value="Search By Title" />
</form>

@foreach(var post in Model)
{
    <div>
        <h3>@post.Title</h3>
        <p>@post.Body</p>
        <hr />
    </div>
}